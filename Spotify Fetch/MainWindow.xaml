<Window x:Class="Spotify_Fetch.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Spotify_Fetch"
        mc:Ignorable="d"
       
        Title="Spotify Fetch" Height="600" Width="800">
    <Window.TaskbarItemInfo>
        <TaskbarItemInfo x:Name="taskBarItemInfo" />
    </Window.TaskbarItemInfo>
    <StackPanel x:Name="visuals">
        <TextBlock Text="0/0" TextAlignment="Center" x:Name="downloadProgress"/>
    </StackPanel>
</Window>
